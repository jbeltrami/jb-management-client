<form id="update-client">
  <legend>Update client</legend>
  <input class="form-control clientId"name="client[id]" type="text" placeholder="Client ID">
  <input class="form-control"name="client[first_name]" type="text" placeholder="First Name">
  <input class="form-control"name="client[family_name]" type="text" placeholder="Family Name">
  <input class="form-control"name="client[born_on]" type="date" placeholder="DOB yyyy/mm/dd">
  <input class="form-control"name="client[email]" type="text" placeholder="email">

  <input class="btn btn-md btn-success btn-block" type="submit" name="submit" value="Update client!">
</form>

